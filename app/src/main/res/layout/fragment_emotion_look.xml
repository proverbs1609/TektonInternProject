<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFDDA7">

    <Button
        android:id="@+id/look_button1"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="30dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="80dp"
        android:text="눈을 치뜬다" />

    <Button
        android:id="@+id/look_button2"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_alignTop="@+id/look_button1"
        android:layout_toEndOf="@+id/look_button1"
        android:layout_toRightOf="@+id/look_button1"
        android:text="눈을 동그랗게 뜬다" />

    <Button
        android:id="@+id/look_button3"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_alignLeft="@+id/look_button1"
        android:layout_alignStart="@+id/look_button1"
        android:layout_below="@+id/look_button1"
        android:layout_marginTop="10dp"
        android:text="눈을 깜빡인다" />

    <Button
        android:id="@+id/look_button4"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_alignLeft="@+id/look_button2"
        android:layout_alignStart="@+id/look_button2"
        android:layout_below="@+id/look_button2"
        android:layout_marginTop="10dp"
        android:text="눈을 가늘게 뜬다" />

    <Button
        android:id="@+id/look_button5"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_alignLeft="@+id/look_button3"
        android:layout_alignStart="@+id/look_button3"
        android:layout_below="@+id/look_button3"
        android:layout_marginTop="10dp"
        android:text="시선을 고정한다" />

    <Button
        android:id="@+id/look_button6"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_alignLeft="@+id/look_button4"
        android:layout_alignStart="@+id/look_button4"
        android:layout_below="@+id/look_button4"
        android:layout_marginTop="10dp"
        android:text="눈을 피한다" />

    <Button
        android:id="@+id/look_button7"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_alignLeft="@+id/look_button5"
        android:layout_alignStart="@+id/look_button5"
        android:layout_below="@+id/look_button5"
        android:layout_marginTop="10dp"
        android:text="코를 핥는다" />

    <Button
        android:id="@+id/look_button8"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_alignLeft="@+id/look_button6"
        android:layout_alignStart="@+id/look_button6"
        android:layout_below="@+id/look_button6"
        android:layout_marginTop="10dp"
        android:text="입꼬리를 올린다" />

    <Button
        android:id="@+id/look_button9"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_alignLeft="@+id/look_button7"
        android:layout_alignStart="@+id/look_button7"
        android:layout_below="@+id/look_button7"
        android:layout_marginTop="10dp"
        android:text="입을 가볍게 벌린다" />

    <Button
        android:id="@+id/look_button10"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_alignLeft="@+id/look_button8"
        android:layout_alignStart="@+id/look_button8"
        android:layout_below="@+id/look_button8"
        android:layout_marginTop="10dp"
        android:text="귀를 쫑긋하게 세운다" />

    <Button
        android:id="@+id/look_button11"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_alignLeft="@+id/look_button9"
        android:layout_alignStart="@+id/look_button9"
        android:layout_below="@+id/look_button9"
        android:layout_marginTop="10dp"
        android:text="귀를 뒤로 젖힌다" />

    <Button
        android:id="@+id/look_button12"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_alignLeft="@+id/look_button10"
        android:layout_alignStart="@+id/look_button10"
        android:layout_below="@+id/look_button10"
        android:layout_marginTop="10dp"
        android:text="송곳니를 드러낸다" />
</RelativeLayout>